<?xml version="1.0" encoding="utf-8"?>
<odoo>
  <record id="channel_hr_import" model="queue.job.channel">
    <field name="name">hr_import</field>
    <field name="parent_id" ref="queue_job.channel_root"/>
  </record>

  <record id="job_function_create_employees" model="queue.job.function">
    <field name="model_id" ref="hr.model_hr_employee"/>
    <field name="method">_job_create_employees_from_rows</field>
    <field name="channel_id" ref="channel_hr_import"/>
    <field name="retry_pattern" eval="{1: 10, 2: 30, 3: 60, 4: 300}"/>
  </record>


</odoo>
